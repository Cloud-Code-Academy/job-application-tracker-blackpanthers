<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidFollowupDate</fullName>
    <active>true</active>
    <errorConditionFormula>Follow_up_date__c &lt; Application_Date__c</errorConditionFormula>
    <errorDisplayField>Follow_up_date__c</errorDisplayField>
    <errorMessage>The follow-up date cannot be earlier than the application date</errorMessage>
</ValidationRule>
